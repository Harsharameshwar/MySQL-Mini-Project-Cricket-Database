<%- include("../partials/tableheader"); -%>
<h2 class="heading-section">Table <%= tablename %> </h2>
</div>
</div>
<div class="row">
<div class="col-md-12">
<div class="table-wrap">
<table class="table table-striped">
<thead>
    <tr>
        <th>captain_id</th>
        <th>captain_name</th>
        <th>team_id</th>
        <th>player_id</th>
        <th>year_of_captaincy</th>
        <th>no_of_wins</th>
    </tr>
</thead>
<tbody>
    
    <%  posts.forEach(function(post){ %>
    <%  post.forEach(function(pos){ %>

        <tr>
    
            <td><%= pos?.captain_id%></td>
            <td><%= pos?.captain_name%></td>
            <td><%= pos?.team_id%></td>
            <td><%= pos?.player_id%></td>
            <td><%= pos?.year_of_captaincy%></td>
            <td><%= pos?.no_of_wins%></td>
        </tr>
  <%  })  %>
 <% }) %>

<%- include("../partials/tablefooter"); -%>

